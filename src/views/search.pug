extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 "#{searchingBy}" 검색 결과
    .search__videos
        if videos.length === 0
            h5 검색 결과가 없습니다
        each item in videos
            +videoBlock({
                id: item.id,
                title: item.title,
                views: item.views,
                videoFile: item.fileUrl
            })